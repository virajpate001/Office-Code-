   // Function to populate city dropdown based on selected state
   function populateCities() {
      const selectedState = stateDropdown.value;
      const cities = citiesByState[selectedState] || [];

      cityDropdown.innerHTML = '<option value="">Select a City</option>';
      cities.forEach(city => {
         const option = document.createElement('option');

         option.value = city;

         option.textContent = city;

         cityDropdown.appendChild(option);

      });
   }

   // Event listener for state dropdown change
   stateDropdown.addEventListener('change', populateCities);

   // Event listener for submit button click
   document.getElementById('submitBtn').addEventListener('click', function () {
      const selectedState = stateDropdown.value;
      const selectedCity = cityDropdown.value;
      let showData = document.getElementById("shoData");
     
       if (!selectedState == '' && !selectedCity == '') {
          showData.classList.add("showData");
       }
       else{
         showData.classList.remove("showData");
       }

      // Form a key based on selected options
      const key = `${selectedState}_${selectedCity}`;
      

      // Fetch company information based on the key
      const info = companyInfo[key];


      // Display company information
      if (info) {
         // alert(`Company: ${info.company}\nAddress: ${info.address}\nState: ${info.state}\nName: ${info.name}`);

         let data = document.getElementById("shoData");

         data.innerHTML = `  <div class="cdiv"><h3 class="dname">${info.dealername}</h3><p class="addres">${info.address}</p></div>`;

      } else {
         alert('No information found for the selected options.');
      }
   });
});
 </script>
      
      
      
      
      
      
      
      
